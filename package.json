{
  "name": "@zuhair-cs/cs-mcp",
  "version": "0.0.1",
  "main": "./dist/bundle.js",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "bin": {
    "contentstack-mcp-server": "./bin/contentstack-mcp.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "node build.js && chmod +x bin/contentstack-mcp.js",
    "clean": "rm -rf dist",
    "lint": "eslint src",
    "lint:fix": "eslint --fix",
    "dev": "nodemon --watch src -e ts --exec 'npm run build'",
    "prettier": "prettier src --check",
    "prettier:fix": "prettier src --write",
    "format": "npm run prettier:fix && npm run lint:fix",
    "test": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "description": "",
  "repository": {
        "type": "git",
        "url": "https://github.com/ZuhairAhmed-cs/cs-mcp.git"
    },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.11.0",
    "axios": "^1.9.0",
    "dotenv": "^16.5.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/js": "^9.26.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.14.1",
    "esbuild": "^0.25.3",
    "eslint": "^9.26.0",
    "eslint-config-prettier": "^10.1.3",
    "eslint-plugin-only-warn": "^1.1.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "prettier": "^3.5.3",
    "ts-jest": "^29.3.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.32.0"
  },
  "lint-staged": {
    "*.{js,ts,json,md}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
